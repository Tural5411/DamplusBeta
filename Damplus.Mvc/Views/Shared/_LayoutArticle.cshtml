<!DOCTYPE html>
<html lang="az">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link href="/damplus/assets/images/logo/DumPlusNewLogo.png" rel="icon">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="keywords" content="@ViewBag.Tags" />
    <meta name="image" content="https://damplus.az/img/@ViewBag.image" />
    <meta name="twitter:image" content="https://damplus.az/img/@ViewBag.image" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="@@damplus" />
    <meta name="twitter:creator" content="@@damplus">
    <meta name="twitter:title" content="@ViewBag.Title" />
    <meta name="twitter:description" content="@ViewBag.Description" />
    <meta property="og:type" content="article" />
    <meta property="og:description" content="@ViewBag.Description">
    <meta property="og:url" content="https://damplus.az/@ViewBag.seolink" />
    <meta property="og:site_name" content="damplus" />
    <meta property="og:locale" content="az_AZ" />
    <meta property="og:title" name="og:title" content="@ViewBag.Title" />
    <meta property="og:image" content="https://damplus.az/img/@ViewBag.image" />
    <meta property="og:image:type" content="image/webp" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="thumbnail" content="https://damplus.az/img/@ViewBag.image" />
    <meta name="msapplication-TileImage" content="https://damplus.az/img/@ViewBag.image" />
    <meta name="msapplication-TileColor" content="#ffffff" />
    <link rel="canonical" href="https://damplus.az/@ViewBag.seolink">
    <meta name="author" content="@ViewBag.Author">
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Barlow:400,500,600,700%7cRoboto:400,500,700&amp;display=swap">
    <link rel="stylesheet" href="~/damplus/assets/css/libraries.css">
    <link rel="stylesheet" href="~/damplus/assets/css/style.css">
</head>

<body>
    <div class="wrapper">
        <div class="preloader">
            <div class="loading"><span></span><span></span><span></span><span></span></div>
        </div><!-- /.preloader -->
        <!-- =========================
            Header
        =========================== -->
        @await Component.InvokeAsync("Header") <!-- /.Header -->
        @RenderBody()

        <!-- ========================
          Footer
        ========================== -->
        @await Component.InvokeAsync("Footer") <!-- /.Footer -->
        <button id="scrollTopBtn"><i class="fa fa-long-arrow-up"></i></button>

        <div class="search-popup">
            <i class="search__popup-close">&times;</i>
            <form class="search__popup-form">
                <input type="text" class="search__input" method="get" asp-controller="Article" asp-action="Search" placeholder="Axtarış">
                <button class="module__search-btn"><i class="fa fa-search"></i></button>
            </form>
        </div><!-- /. search-popup -->

    </div><!-- /.wrapper -->
    <script>window.whatsformId = '1mhzpP';
        window.whatsformMessage = 'WhatsApp';</script>
    <script async src="https://whatsform.com/launcher.js"></script>

    <script src="~/damplus/assets/js/jquery-3.5.1.min.js"></script>
    <script src="~/damplus/assets/js/plugins.js"></script>
    <script src="~/damplus/assets/js/main.js"></script>
</body>
</html>